{
	"version": 2,
	"builds": [
		{
			"src": "index.js",
			"use": "@now/node",
			"timeout": 15
		},
		{
			"src": "/api/telegram.js",
			"use": "@now/node",
			"timeout": 15
		}
	],
	"routes": [
		{
			"src": "/api/telegram",
			"dest": "/api/telegram.js"
		},
		{
			"src": "/(.*)",
			"dest": "index.js"
		}
	]
}
